//https://www.geeksforgeeks.org/problems/sum-of-fifth-powers-of-the-first-n-natural-numbers3415/1

// User function template for C++

class Solution {
  public:
    long long sumOfFifthPowers(long long N) {
        // code here
        long long sum=0;
        for (long long i=1;i<=N;i++){
            sum = sum+ i*i*i*i*i;
        }
        return sum;
    }
};
